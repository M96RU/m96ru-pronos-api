<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="0001" author="manu">
        <createTable tableName="matches">
            <column name="id" type="varchar(50)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="begin" type="timestamp with time zone">
                <constraints nullable="false"/>
            </column>
            <column name="home" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="away" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="h2h_1" type="float">
                <constraints nullable="false"/>
            </column>
            <column name="h2h_x" type="float">
                <constraints nullable="false"/>
            </column>
            <column name="h2h_2" type="float">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>